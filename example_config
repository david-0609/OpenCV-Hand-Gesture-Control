[GESTURE1]
name = 2-Finger-Up-Swipe
fingers_up = 2
direction = u
action = pageup
action_type = keyboard

[GESTURE2]
name = three_finger_left
fingers_up = 3
direction = l
action = f12 
action_type = keyboard
# Toggle dropdown terminal  

[GESTURE3]
name = three_finger_right
fingers_up = 3
direction = r
action = ctrl,alt,t 
action_type = keyboard
# Opens terminal window

[GESTURE4]
name = five_finger_right
fingers_up = 5
direction = r
action = firefox 
action_type = command 
# Opens firefox 

[GESTURE5]
name = five_finger_left
fingers_up = 5
direction = l
action = ctrl,shift,home
action_type = keyboard
# Return to desktop

